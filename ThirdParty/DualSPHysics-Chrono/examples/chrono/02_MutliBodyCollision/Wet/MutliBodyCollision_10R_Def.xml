<?xml version="1.0" encoding="UTF-8" ?>
<case app="GenCase [Beta] v5.0.254 (10-03-2022)" date="13-03-2022 01:00:05">
    <casedef requiredversion="v5.0.254">
        <constantsdef>
            <gravity x="0" y="0" z="-9.81" comment="Gravitational acceleration" units_comment="m/s^2" />
            <rhop0 value="1000" comment="Reference density of the fluid" units_comment="kg/m^3" />
            <hswl value="0" auto="true" comment="Maximum still water level to calculate speedofsound using coefsound" units_comment="metres (m)" />
            <gamma value="7" comment="Polytropic constant for water used in the state equation" />
            <speedsystem value="0" auto="true" comment="Maximum system speed (by default the dam-break propagation is used)" />
            <coefsound value="20" comment="Coefficient to multiply speedsystem" />
            <speedsound value="100" auto="true" comment="Speed of sound to use in the simulation (by default speedofsound=coefsound*speedsystem)" />
            <coefh value="1.2" comment="Coefficient to calculate the smoothing length (h=coefh*sqrt(3*dp^2) in 3D)" />
            <cflnumber value="0.2" comment="Coefficient to multiply dt" />
        </constantsdef>
        <mkconfig boundcount="440" fluidcount="9" />
        <geometry>
            <!-- User-defined variables  -->
            <predefinition>
                <!-- SPH resolution -->
                <newvarcte L="0.010" />
                <newvarcte np="15" _rem="np:dp = 5:0.012, 10:0.006, 15:0.004, 20:0.003, 24:0.0025" />
                <newvarcte Dp="L/np" Dpm="Dp/2" Dpr="L/100" Dpr2="Dpr*2" />
                <!-- Tank and fluid definition -->
                <newvarcte tsx="0.26" tsy="1" tsz="0.26" />
                <newvarcte fsx="0.06" fsy="1" fsz="0.12" />
                <!-- Wall definition -->
                <newvarcte wpx="L/2" wpz="L/2" wsy="L/2" wsz="L/2" />                
                <newvarcte tblocks="4" _rem="1:box, 2:cilindro-8, 3:cilindro-18, 4:cilindro-32" />
                <newvarcte bs="L" />
                <newvarcte bnx="5" bnz="6" />
                <newvarcte bsep="1.0*Dp" />
                <newvarcte width="0.10" />
                <newvarcte shiftzFirst="0.25*L" />
                <newvarcte wtotmass="3.141519*bs*bs/4*width*2700" />
                <newvarstrcte ftmkbrange="11-65" _rem="Mk range for floatings" />
            </predefinition>
            <definition dp="#Dp" units_comment="metres (m)">
                <pointref x="0" y="0" z="0" />
                <pointmin x="#-L" y="#-width" z="#-L" />
                <pointmax x="2" y="#width" z="1" />
            </definition>
            <commands>
                <list name="BlockCyl" printcall="false">
                    <setdpctes ctecylindernumsides="8" if="tblocks==2" />
                    <setdpctes ctecylindernumsides="18" if="tblocks==3" />
                    <setdpctes ctecylindernumsides="32" if="tblocks==4" />
                    <!-- for particles -->
                    <setactive drawpoints="1" drawshapes="0" />
                    <setfrdrawmode auto="true" />
                    <drawcylinder radius="#bs/2-Dpm-bsep">
                        <point x="#bs/2" y="#-width/2" z="#bs/2" />
                        <point x="#bs/2" y="#width/2" z="#bs/2" />
                    </drawcylinder> 
                    <!-- for collision faces -->
                    <setactive drawpoints="0" drawshapes="1" />
                    <drawcylinder radius="#bs/2-Dpr">
                        <point x="#bs/2" y="#-width/2" z="#bs/2" />
                        <point x="#bs/2" y="#width/2" z="#bs/2" />
                    </drawcylinder>
                    <!-- Displacement for the next block -->
                    <move x="#bs" y="0" z="0" />
                    <setmknextbound next="true" />
                </list>	
				<!-- Here we define the normal geometry per each shape -->				
				<list name="BlockCylNorm" printcall="false">
                    <setdpctes ctecylindernumsides="32"/>
                    <drawcylinder radius="#bs/2-bsep" >
                        <point x="#bs/2" y="#-width/2" z="#bs/2" />
                        <point x="#bs/2" y="#width/2" z="#bs/2" />
						<layers vdp="0" />
                    </drawcylinder>
					<setfrdrawmode auto="false" />		
					<!-- Displacement for the next block -->
                    <move x="#bs" y="0" z="0" />
                    <setmknextbound next="true" />					
                </list>
                <mainlist>
					<setactive drawpoints="0" drawshapes="1"/>
                    <setshapemode>actual | bound</setshapemode>					
					<!-- Tank -->                    
					<setnormalinvert invert="true" />                    
                    <setmkbound mk="0" />
                    <drawbox>
                        <boxfill>all^top</boxfill>
                        <point x="0" y="#-width/2" z="0" />
                        <size x="#tsx" y="#width" z="#tsz" />
                        <layers vdp="0.5" />
                    </drawbox>					
					<setnormalinvert invert="false" />					
					<!-- mobile gate -->
					<setmkbound mk="100" />
                    <drawbox>
                        <boxfill>left</boxfill>
                        <point x="#6*bs+2*Dp-Dpm" y="#-width/2" z="0" />
                        <size x="#3*Dp" y="#width" z="0.14" />
					</drawbox>					
					<!-- Line 1 -->
                    <setmkbound mk="11" />
					<move x="0" y="0" z="#shiftzFirst" />
                    <matrixsave />
					<runlist name="BlockCylNorm" times="6" />
					<matrixload />
					<!-- Line 2 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="5" />
                    <matrixload />
					<!-- Line 3 -->
					<move x="#-bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="6" />
                    <matrixload />
					<!-- Line 4 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="5" />
                    <matrixload />
					<!-- Line 5 -->
					<move x="#-bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="6" />
                    <matrixload />
					<!-- Line 6 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="5" />
                    <matrixload />
					<!-- Line 7 -->
					<move x="#-bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="6" />
                    <matrixload />
					<!-- Line 8 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="5" />
                    <matrixload />	
					<!-- Line 9 -->
					<move x="#-bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="6" />
                    <matrixload />
					<!-- Line 10 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCylNorm" times="5" />
                    <matrixload />	
					<shapeout file="hdp" />
                    <resetdraw />
					
                    <setshapemode>actual | bound</setshapemode>
                    <setdrawmode mode="full" />
                    <setfrdpoverlap overlapdp="0.5" />
                    <!-- Fluid -->
                    <setmkfluid mk="0" />
                    <drawbox>
                        <boxfill>solid</boxfill>
                        <point x="0" y="#-width/2" z="0" />
                        <size x="#fsx+3*Dp" y="#width" z="#fsz" />
                    </drawbox>
                    <!-- Tank for particles -->
                    <setactive drawpoints="1" drawshapes="0" />
                    <setmkbound mk="0" />
                    <drawbox>
                        <boxfill>all^top</boxfill>
                        <point x="0" y="#-width/2" z="0" />
                        <size x="#tsx" y="#width" z="#tsz" />
                        <layers vdp="1,2,3,4" />
                    </drawbox>
					<shapeout file="" />
                    <!-- Tank for Chrono collisions -->
                    <setactive drawpoints="0" drawshapes="1" />
					<drawbox>
                        <boxfill>top</boxfill>
                        <point x="0" y="#-1.1*width/2" z="#-3*Dp" />
                        <size x="#tsx" y="#1.1*width" z="#3*Dp" />
                    </drawbox>
					<drawbox>
                        <boxfill>right</boxfill>
                        <point x="#-3*Dp" y="#-1.1*width/2" z="0" />
                        <size x="#3*Dp" y="#1.1*width" z="#tsz" />
                    </drawbox>
					<drawbox>
                        <boxfill>left</boxfill>
                        <point x="#tsx" y="#-1.1*width/2" z="0" />
                        <size x="#3*Dp" y="#1.1*width" z="#tsz" />
                    </drawbox>					
                    <shapeout file="tank" />					
					<!-- mobile gate -->
					<setmkbound mk="100" />
					<setdrawmode mode="full" />
					<setactive drawpoints="1" drawshapes="1" />
                    <drawbox>
                        <boxfill>solid</boxfill>
                        <point x="#6*bs+2*Dp" y="#-width/2" z="0" />
                        <size x="#3*Dp" y="#width" z="0.14" />
                    </drawbox>					
                    <!-- Blocks -->
                    <setdrawmode mode="full" />
                    <!-- Line 1 -->
                    <setmkbound mk="11" />
					<move x="0" y="0" z="#shiftzFirst" />
                    <matrixsave />                  
                    <runlist name="BlockCyl" times="6" />                    
                    <matrixload />						
					<!-- Line 2 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="5" />
                    <matrixload />
					<!-- Line 3 -->
					<move x="#-bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="6" />
                    <matrixload />
					<!-- Line 4 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="5" />
                    <matrixload />
					<!-- Line 5 -->
					<move x="#-bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="6" />
                    <matrixload />
					<!-- Line 6 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="5" />
                    <matrixload />	
					<!-- Line 7 -->
					<move x="#-bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="6" />
                    <matrixload />
					<!-- Line 8 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="5" />
                    <matrixload />		
					<!-- Line 9 -->
					<move x="#-bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="6" />
                    <matrixload />
					<!-- Line 10 -->
                    <move x="#bs/2" y="0" z="#0.8660254*bs+shiftzFirst" />
					<matrixsave />                    
                    <runlist name="BlockCyl" times="5" />
                    <matrixload />					
                    <shapeout file="" reset="true" />
                </mainlist>
            </commands>
        </geometry>
        <floatings>
            <floating mkbound="$[ftmkbrange]" >
                <massbody value="#wtotmass" />
				<translationDOF x="1" y="0" z="1" comment="Use 0 to restrict Degrees Of Freedon for translation (default=(1,1,1))" />
                <rotationDOF x="0" y="1" z="0" comment="Use 0 to restrict Degrees Of Freedon for rotation (default=(1,1,1))" />
			</floating>
			<floating mkbound="100" >
				<massbody value="1" />
                <translationDOF x="1" y="0" z="1" comment="Use 0 to restrict Degrees Of Freedon for translation (default=(1,1,1))" />
                <rotationDOF x="1" y="0" z="1" comment="Use 0 to restrict Degrees Of Freedon for rotation (default=(1,1,1))" />
				<linearvel units_comment="m/s">
                    <vel time="0" x="0" z="0" />
                    <vel time="0.40" x="0" z="0" />
                    <vel time="0.40" x="0.01" z="2" />
                    <vel time="0.50" x="0.01" z="2" />
                    <vel time="0.50" x="0" z="0" />                    
                </linearvel>
			</floating>
        </floatings>
		<normals active="true">
            <distanceh value="3.0" comment="Maximum distance (H*distanceh) to compute normals data (default=2)" />
            <ftsupport value="1" comment="Enables support for floating bodies (default=0)" />
            <geometryfile file="[CaseName]_hdp_Actual.vtk" comment="File with boundary geometry (VTK format)" />
        </normals>
		<properties>
		<propertyfile file="Floating_Materials.xml" path="materials" />
            <links>
                <link mkbound="0" property="pvc" />
				<link mkbound="$[ftmkbrange]" property="aluminium"  />
				<link mkbound="100" property="pvc"  />
			</links>
	</properties>
    </casedef>
    <execution>
        <special>
            <chrono>
                <savedata value="0.001" comment="Saves CSV with data exchange for each time interval (0=all steps)" />
                <schemescale value="1" comment="Scale used to create the initial scheme of Chrono objects (default=1)" />
                <collision activate="true">
                    <distancedp value="0.05" comment="Allowed collision overlap according Dp (default=0.5)" />
                    <contactmethod value="0" comment="Contact method type. 0:NSC (Non Smooth Contacts), 1:SMC (SMooth Contacts). (default=0)" />
                </collision>
				<bodyfloating id="block" mkbound="$[ftmkbrange]" modelfile="AutoActual" />                
                <bodyfixed id="tank" mkbound="0" modelfile="AutoActual"  />
                <bodyfloating id="gate" mkbound="100" modelfile="AutoActual" />
            </chrono>
        </special>
        <parameters>
			<parameter key="Boundary" value="2" comment="Boundary method 1:DBC, 2:mDBC (default=1)" />            
            <parameter key="StepAlgorithm" value="2" comment="Step Algorithm 1:Verlet, 2:Symplectic (def=1)" />            
            <parameter key="Kernel" value="2" comment="Interaction Kernel 1:Cubic Spline, 2:Wendland (def=1)" />
            <parameter key="ViscoTreatment" value="2" comment="Viscosity Formulation 1:Artificial, 2:Laminar+SPS (def=1)" />
            <parameter key="Visco" value="1.30e-6" comment="Viscosity value" />
            <parameter key="ViscoBoundFactor" value="1" comment="Multiply viscosity value with boundary (default=1)" />
            <parameter key="DensityDT" value="3" comment="Density Diffusion Term 0:None, 1:Molteni, 2:Fourtakas, 3:Fourtakas(full) (default=0)" />
            <parameter key="DensityDTvalue" value="0.1" comment="DDT value (default=0.1)" />
            <parameter key="Shifting" value="0" comment="Shifting mode 0:None, 1:Ignore bound, 2:Ignore fixed, 3:Full (default=0)" />
            <parameter key="ShiftCoef" value="-2" comment="Coefficient for shifting computation (default=-2)" />
            <parameter key="ShiftTFS" value="0" comment="Threshold to detect free surface. Typically 1.5 for 2D and 2.75 for 3D (default=0)" />
            <parameter key="RigidAlgorithm" value="3" comment="Rigid Algorithm 0:collision-free, 1:SPH, 2:DEM, 3:Chrono (default=1)" />
            <parameter key="FtPause" value="0" comment="Time to freeze the floatings at simulation start (warmup) (def=0)" />
            <parameter key="DtIni" value="0" comment="Initial time step. Use 0 to defult use (default=h/speedsound)" units_comment="seconds" />
            <parameter key="DtMin" value="1e-7" comment="Minimum time step. Use 0 to defult use (default=coefdtmin*h/speedsound)" units_comment="seconds" />
            <parameter key="DtAllParticles" value="0" comment="Velocity of particles used to calculate DT. 1:All, 0:Only fluid/floating (default=0)" />
            <parameter key="TimeMax" value="0.90" comment="Time of simulation" />
            <parameter key="TimeOut" value="0.001" comment="Time between output files" />
            <parameter key="PartsOutMax" value="1" comment="%/100 of fluid particles allowed to be excluded from domain (default=1)" units_comment="decimal" />
            <parameter key="RhopOutMin" value="700" comment="Minimum rhop valid (default=700)" units_comment="kg/m^3" />
            <parameter key="RhopOutMax" value="1300" comment="Maximum rhop valid (default=1300)" units_comment="kg/m^3" />
            <simulationdomain comment="Defines domain of simulation (default=Uses minimun and maximum position of the generated particles)">
                <posmin x="default" y="default" z="default" comment="e.g.: x=0.5, y=default-1, z=default-10%" />
                <posmax x="default + 0.02" y="default" z="default + 100%" />
            </simulationdomain>
        </parameters>
    </execution>
</case>
